ecios: number[] = [15,60,20];\r\n    static tiempo: number[] = [35,10,25];\r\n    static vehiculos: string[] = [\"Colectivo\",\"Auto\",\"Combi\"];\r\n\r\n    static traslado(a:number,b:number){  //Tiempo, Precio Disponible\r\n        let precioF = this.Precio(a);\r\n        let tiempF = this.Tiempo(b);\r\n\r\n\r\n        if(tiempF != null && precioF != null){\r\n            return this.vehiculos[precioF]\r\n        }\r\n        else if(tiempF != null && precioF == null){\r\n            return this.vehiculos[tiempF]\r\n        }\r\n        else if(tiempF == null && precioF != null){\r\n            return this.vehiculos[precioF]\r\n        }\r\n        else{\r\n            return \"Error\";\r\n        }\r\n        \r\n        \r\n    }\r\n\r\n    static Precio(a:number){\r\n        let aux = null\r\n\r\n        for(let i=0;i<3;i++){\r\n            if(a == this.precios[i]){\r\n                aux=i;\r\n            }\r\n        }\r\n        return aux;\r\n\r\n    }\r\n\r\n    static Tiempo(b:number){\r\n        let aux = null\r\n\r\n        for(let i=0;i<3;i++){\r\n            if(b == this.precios[i]){\r\n                aux=i;\r\n            }\r\n        }\r\n        return aux;\r\n    }\r\n\r\n}"]},"_coverageSchema":"1a1c01bbd47fc00a2c39e90264f33305004495a9","hash":"89980369ac60f6a38becb14d871b5bf1501c1a99","contentHash":"af47b5a2762c3bc9ba451d7b8224a3ea40bb9c22bf33ec2bc3f63298432ce76c"}}